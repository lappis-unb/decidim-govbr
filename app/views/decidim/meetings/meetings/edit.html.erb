<div class="br-container-lg">
  <%= render partial: "wizard_aside" %>
  <div class="new-meetings-header">
    <%= render partial: "wizard_header", locals: { is_edit_form: true } %>
  </div>
  <div class="new-meeting-form">
    <%= decidim_form_for(@form, html: { class: "meetings_form new_meeting" }) do |f| %>
      <%= render partial: "form", object: f %>
      <div class="actions">
        <%= f.submit t(".update"), class: "button expanded", data: { disable: true } %>
      </div>
    <% end %>
  </div>
  <div class="columns large-3"></div>
</div>
